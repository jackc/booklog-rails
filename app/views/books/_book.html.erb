<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author %>
  </p>

  <p>
    <strong>Finish date:</strong>
    <%= book.finish_date %>
  </p>

  <p>
    <strong>Format:</strong>
    <%= book.format %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= book.location %>
  </p>

</div>
